<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template inherit_id="account.report_invoice_document" id="inherited_invoice_report_template">
        <xpath expr="//t/div[hasclass('page')]/table/thead/tr/th[2]" position="after">
            <th name="expiry_date" class="text-left">Exp Date</th>
        </xpath>
        <xpath expr="//t/div[hasclass('page')]/table/thead/tr/th[2]" position="after">
            <th name="lot_ids" class="text-left">Lot No.</th>
        </xpath>
        <xpath expr="//t/div[hasclass('page')]/table/tbody/t[3]/tr[1]/t[1]/td[2]" position="after">
            <td name="td_quantity" class="text-left">
                <span t-field="line.expiry_date_of_end_life_date"/>
            </td>
        </xpath>
      
        <xpath expr="//t/div[hasclass('page')]/table/tbody/t[3]/tr[1]/t[1]/td[2]" position="after">
            <td name="td_quantity" class="text-left">
            <span t-field="line.lot_ids"/>
            </td>
        </xpath>

        <xpath expr="//table[@name='invoice_snln_table']" position="replace">
            <!-- Your customized table content goes here -->
            <!-- For example, you can add or remove table rows, columns, or modify existing ones -->
            <table class="table table-sm" style="width: 100%;" name="invoice_snln_table">
                <!-- Customized table content -->
                <!-- Add or remove rows and columns as per your requirement -->
            </table>
        </xpath>
    </template>


</odoo>
